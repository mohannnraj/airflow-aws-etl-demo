# Airflow admin user
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=admin

# AWS (fill in in your local .env only)
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY_ID
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_ACCESS_KEY
AWS_DEFAULT_REGION=ap-southeast-2

# RDS connection string (fill actual endpoint in your local .env)
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:YOUR_DB_PASSWORD@your-rds-endpoint.rds.amazonaws.com:5432/airflow

# Airflow executor
AIRFLOW__CORE__EXECUTOR=LocalExecutor

# Metrics (StatsD)
AIRFLOW__METRICS__STATSD_ON=True
AIRFLOW__METRICS__STATSD_HOST=statsd-exporter
AIRFLOW__METRICS__STATSD_PORT=9125
